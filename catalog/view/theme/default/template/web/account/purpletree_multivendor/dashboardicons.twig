                        <h6 class="tab_name">
                            {{ text_Bulk_product_upload }}
                        </h6>
                    </a>
                </div>
                <div></div>
            </div>
        </div>
        <hr/>
        <div class="section_dashboard_one">
            <div class="section_dashboard_title">
                <h4>
                    {{ text_Seller_Payments }}
