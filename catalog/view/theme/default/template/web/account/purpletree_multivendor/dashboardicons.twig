{{ header }}
<div class="container" id="close_nav">
    <ul class="pts-breadcrumb breadcrumb">
        {% for breadcrumb in breadcrumbs %}
            <li class="breadcrumb-item">
                <a href="{{ breadcrumb.href }}">
                    {{ breadcrumb.text }}
                </a>
            </li>
        {% endfor %}
    </ul>
    {% if error_warning %}
        <div class="alert alert-danger">
            <i class="fa fa-exclamation-circle"></i>
            {{ error_warning }}
            <button type="button" class="close" data-dismiss="alert">
                &times;
                                                                                            
                                                                            
                                                            
                                            
                            
            </button>
        </div>
    {% endif %}
    <div class="section_dashboard_area">
        <div class="heading_area">
            <h1 class="dashboard_title">
                {{ heading_title }}
            </h1>
            <div class="toggle-button" id="toggle_btn">
                Seller Option
                            
            </div>
        </div>
        <div class="section_dashboard_one">
            <div class="section_dashboard_title">
                <h4>
                    {{ text_Seller_Account }}
                </h4>
            </div>
            <div class="section_tabs">
                <div class="tab_items">
                    <a href="{{ sellerorder }}" class="tab_flex">
                        <div class="tab_iocn">
                            <iconify-icon icon="emojione-monotone:shopping-cart" flip="horizontal"></iconify-icon>
                        </div>
                        <h6 class="tab_name">
                            {{ text_Orders }}
                        </h6>
                        {% if not totalorders == 0 %}
                            <span class="pts-counting-1">
                                {{ totalorders }}
                            </span>
                        {% endif %}
                    </a>
                </div>
                <div class="tab_items">
                    <a href="{{ sellerenquiries }}" class="tab_flex">
                        <div class="tab_iocn">
                            <iconify-icon icon="bxs:message-alt-dots"></iconify-icon>
                        </div>
                        <h6 class="tab_name">
                            {{ text_sellerenquiries }}
                        </h6>
                        {% if not totaladminmessages==0 %}
                            <span class="pts-counting-1">
                                {{ totaladminmessages }}
                            </span>
                        {% endif %}
                    </a>
                </div>
                <div class="tab_items">
                    <a href="{{ sellerenquiry }}" class="tab_flex">
                        <div class="tab_iocn">
                            <iconify-icon icon="ant-design:question-circle-filled"></iconify-icon>
                        </div>
                        <h6 class="tab_name">
                            {{ text_Customer_Enquiries }}
                        </h6>
                        {% if not totalenqures==0 %}
                            <span class="pts-counting-1">
                                {{ totalenqures }}
                            </span>
                        {% endif %}
                    </a>
                </div>
                {% if subscriptionplan %}
                    <div class="tab_items">
                        <a href="{{ subscriptionplan }}" class="tab_flex">
                            <div class="tab_iocn">
                                <img src="https://cdn-icons-png.flaticon.com/512/3187/3187902.png" class="img_fixed img-responsive" />
                            </div>
                            <h6 class="tab_name mt-5">
                                {{ text_Subscription_plan }}
                            </h6>
                        </a>
                    </div>
                {% endif %}
                <div class="tab_items">
                    <a href="{{ sellerreview }}" class="tab_flex">
                        <div class="tab_iocn">
                            <iconify-icon icon="carbon:review"></iconify-icon>
                        </div>
                        <h6 class="tab_name">
                            {{ text_reviews }}
                        </h6>
                    </a>
                </div>
                <div class="tab_items">
                    <a href="{{ downloadsitems }}" class="tab_flex">
                        <div class="tab_iocn">
                            <iconify-icon icon="akar-icons:cloud-download"></iconify-icon>
                        </div>
                        <h6 class="tab_name">
                            {{ text_Manage_Downloads }}
                        </h6>
                    </a>
                </div>
                <div></div>
            </div>
        </div>
        <hr/>
        <div class="section_dashboard_one">
