	max-width:500px;
	width:100%;
	margin:0 auto;

}

.padding_all{
	padding: 0 2rem;
}.woocommerce-account .addresses .title:after, .woocommerce-account .addresses .title:before, .woocommerce-account .woocommerce:after, .woocommerce-account .woocommerce:before{
	display:none;
