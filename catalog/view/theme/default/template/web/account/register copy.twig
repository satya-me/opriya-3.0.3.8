	<body class="page woocommerce-account woocommerce-page h-100 w-100 ">

		<div class="entry-content">
			<div class="d-flex jcc">
				<a href="{{ home }}">
					<img src="{{assets}}images/logos/logo-index3.png" alt="">
				</a>
			</div>
			<div class="woocommerce card mt-4">
				<div class="wrapper_log padding_all" id="customer_login">
					<div class="loginWrapper" id="loginWrapper">
						<div class="text-center padding_top">
							<h2>Register</h2>
						</div>
						<form action="{{ action }}" method="post" enctype="multipart/form-data" class="login">
							<p class="form-row form-row-wide">
								<label for="input-firstname">
									{{ entry_firstname }}
									<span class="required">*</span>
								</label>

								{# <input type="text" class="input-text" name="username" id="username" /> #}
								<input type="text" name="firstname" value="{{ firstname }}" placeholder="{{ entry_firstname }}" id="input-firstname" class="form-control">
								{% if error_firstname %}
									<div class="text-danger">{{ error_firstname }}</div>
								{% endif %}
							</p>

							<p class="form-row form-row-wide">
								<label for="password">
								{{ entry_lastname }}
									<span class="required">*</span>
								</label>

								{# <input class="input-text" type="password" name="password" id="password" /> #}
								<input type="text" name="lastname" value="{{ lastname }}" placeholder="{{ entry_lastname }}" id="input-lastname" class="form-control">
								{% if error_lastname %}
									<div class="text-danger">{{ error_lastname }}</div>
								{% endif %}
							</p>

							<p class="form-row form-row-wide">
								<label for="password">
								{{ entry_email }}
									<span class="required">*</span>
								</label>

								{# <input class="input-text" type="password" name="password" id="password" /> #}
								<input type="email" name="email" value="{{ email }}" placeholder="{{ entry_email }}" id="input-email" class="form-control">
								{% if error_email %}
									<div class="text-danger">{{ error_email }}</div>
								{% endif %}
							</p>

							<p class="form-row form-row-wide">
								<label for="password">
								{{ entry_telephone }}
									<span class="required">*</span>
								</label>

								{# <input class="input-text" type="password" name="password" id="password" /> #}
								<input type="tel" name="telephone" value="{{ telephone }}" placeholder="{{ entry_telephone }}" id="input-telephone" class="form-control">
								{% if error_telephone %}
									<div class="text-danger">{{ error_telephone }}</div>
								{% endif %}
							</p>
							<hr/>
							<p class="form-row form-row-wide">
								<label for="password">
									Password
									<span class="required">*</span>
								</label>

								{# <input class="input-text" type="password" name="password" id="password" /> #}
								<input type="password" name="password" value="{{ password }}" placeholder="{{ entry_password }}" id="input-password" class="form-control">
								{% if error_password %}
									<div class="text-danger">{{ error_password }}</div>
								{% endif %}
							</p>
							<p class="form-row form-row-wide">
								<label for="password">
									Password
									<span class="required">*</span>
								</label>

								{# <input class="input-text" type="password" name="password" id="password" /> #}
								<input type="password" name="confirm" value="{{ confirm }}" placeholder="{{ entry_confirm }}" id="input-confirm" class="form-control">
								{% if error_confirm %}
									<div class="text-danger">{{ error_confirm }}</div>
								{% endif %}
							</p>

							{# <p class="form-row">
															<label for="rememberme" class="inline">
																<input name="rememberme" type="checkbox" id="rememberme" value="forever"/>
																Remember me
															</label>
														</p>
							
														<p class="form-row">
															<input type="submit" class="button" value="{{ button_continue }}"/>
														</p> #}

							{{ captcha }}
							{% if text_agree %}
								{% if agree %}
									<p class="form-row">
										<label for="rememberme" class="inline">
											<input name="rememberme" type="checkbox" id="rememberme" value="forever"/>
											{{text_agree}}
										</label>
									</p>
								{% else %}
									<p class="form-row">
										<label for="rememberme" class="inline">
											<input name="rememberme" type="checkbox" id="rememberme" value="forever"/>
											{{text_agree}}
										</label>
									</p>
								{% endif %}
								&nbsp;
								<p class="form-row">
									<input type="submit" class="button" value="{{ button_continue }}"/>
								</p>

							{% else %}
								<p class="form-row">
									<input type="submit" class="button" value="{{ button_continue }}"/>
								</p>
							{% endif %}
						</form>
						<div class="d-flex jcc">
							<p class="mb-0 mr-3">I have account ?</p>
							<a href="{{ login }}">
								<p class="mb-0 text-primary ml-3 link">Login</p>
							</a>
						</div>
					</div>
				</div>
			</div>
		</div>


